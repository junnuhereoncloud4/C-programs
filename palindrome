#include<stdio.h>
int main()
{
    int d,n,rev=0,temp;
    printf("enter n value");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        d=n%10;
        rev=(rev*10)+d;
        n=n/10;
    }
    if(temp==rev)
        printf("%d is a palindrome",temp);
    else
        printf("%d is not a palindrome",temp);
    return 0;
}
